local MakePart = function(c)
	local hold = Instance.new("Part", workspace)
	local bbg =  ColorSequence.new({
		ColorSequenceKeypoint.new(0, c),
		ColorSequenceKeypoint.new(1, c),
	})
	local prt1 = Instance.new("Part", hold)
	prt1.Size = Vector3.new(0, 0, 0)
	prt1.Massless = true
	prt1.Transparency = 1
	prt1.CanCollide = false
	prt1.Anchored = true
	local Attat = Instance.new("Attachment", prt1)
	local prt2 = Instance.new("Part", hold)
	prt2.Size = Vector3.new(0, 0, 0)
	prt2.Transparency = 0
	prt2.CanCollide = false
	prt2.Anchored = true
	prt2.Material = Enum.Material.ForceField
	prt2.Color = c
	prt2.Massless = true
	local Attat2 = Instance.new("Attachment", prt2)
	local semem = Instance.new("Beam", prt1)
	semem.FaceCamera = true
	semem.Color = bbg
	semem.Attachment0 = Attat
	semem.Attachment1 = Attat2
	semem.LightEmission = 16
	semem.LightInfluence = 16
	semem.Width0 = 0.25
	semem.Width1 = 0.25
	semem.Texture = "1"
	semem.TextureSpeed = 5
	semem.TextureLength = 5

	return prt1,prt2
end
function HasGun()
	local plr = game:GetService("Players")[CurrentTarg]
	if plr.Character then
		for i, v in pairs(plr.Character:GetChildren()) do
			if v:FindFirstChild("Ammo") then
				return true
			end
		end
	end
	return false
end
